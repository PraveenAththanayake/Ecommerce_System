{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./types", "./node_modules/@types"],
    "noImplicitAny": false,
    "strictFunctionTypes": false,
    "strictBindCallApply": false
  }
}
